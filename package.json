{
    "name": "contact-keeper",
    "version": "1.0.0",
    "description": "Fullstack MERN app, Create and manage your contacts",
    "main": "server.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix client",
        "clientinstall": "npm install --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sandra-td/contact-keeper.git"
    },
    "author": "Aleksandra",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sandra-td/contact-keeper/issues"
    },
    "homepage": "https://github.com/sandra-td/contact-keeper#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "config": "^3.3.1",
        "express": "^4.17.3",
        "express-validator": "^6.4.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.12"
    },
    "devDependencies": {
        "concurrently": "^5.2.0",
        "nodemon": "^2.0.3"
    }
}
